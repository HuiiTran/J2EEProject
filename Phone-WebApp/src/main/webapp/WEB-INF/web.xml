<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>test</servlet-name>
  	<display-name>test</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.test</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlets</servlet-name>
  	<display-name>RegisterServlets</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.RegisterServlets</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletRegister</servlet-name>
  	<display-name>ServletRegister</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.ServletRegister</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>test</servlet-name>
  	<url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlets</servlet-name>
  	<url-pattern>/RegisterServlets</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletRegister</servlet-name>
  	<url-pattern>/ServletRegister</url-pattern>
  </servlet-mapping>
</web-app>
